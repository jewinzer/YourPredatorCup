GET     /signup                 controllers.HomeController.signup(message: Option[String])

POST    /validate/user          controllers.HomeController.validateUser

POST    /validate/login         controllers.HomeController.validateLogin

GET     /user                   controllers.HomeController.showUserCtches

GET     /logout                 controllers.HomeController.logout

POST    /insert/ctch            controllers.HomeController.insertCtch

POST    /delete                 controllers.HomeController.deleteCtch

POST    /duplicate              controllers.HomeController.duplicateCtch

POST    /show                   controllers.HomeController.showCtch

POST    /update                 controllers.HomeController.updateCtch

GET     /env                    controllers.HomeController.env()

GET     /assets/*file           controllers.Assets.versioned(path="/public", file: Asset)